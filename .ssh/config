Protocol 2,1
HashKnownHosts no
NoHostAuthenticationForLocalhost yes
ServerAliveInterval 30
GSSAPIAuthentication no

# multiplex sessions over one connection
ControlMaster auto
ControlPath   ~/.ssh/control/ssh_mux_%h_%p_%r
ControlPersist yes

Include ~/.config/dotfiles/main/ssh/config

ForwardX11 no

Host *
  User seth
